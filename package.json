{
  "name": "din-advent",
  "version": "1.0.0",
  "scripts": {
    "start": "blitz start",
    "studio": "blitz db studio",
    "build": "blitz build",
    "migrate": "blitz db migrate",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:dev": "cypress open --browser chrome",
    "test:watch": "jest --watch",
    "cypress": "cypress run",
    "wait:test": "wait-on http://localhost:3000 && npm run cypress",
    "write:testenv": "node ./scripts/write-env-file.js",
    "test:ci": "npm run write:testenv && npm run migrate && node ./scripts/ci-cypress.js"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "blitz test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/cli": "2.5.1",
    "@prisma/client": "2.5.1",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "10.4.9",
    "@testing-library/react-hooks": "3.4.1",
    "@types/jest": "26.0.10",
    "@types/react": "16.9.46",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "babel-eslint": "10.1.0",
    "blitz": "0.18.0",
    "colors": "1.4.0",
    "concurrently": "5.3.0",
    "cypress-jest-adapter": "0.1.1",
    "eslint": "7.7.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "final-form": "4.20.1",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.6.0",
    "lint-staged": "10.2.11",
    "mailgun.js": "2.0.1",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "prettier": "2.1.0",
    "pretty-quick": "2.0.2",
    "react": "0.0.0-experimental-33c3af284",
    "react-dom": "0.0.0-experimental-33c3af284",
    "react-error-boundary": "2.3.1",
    "react-final-form": "6.5.1",
    "react-test-renderer": "16.13.1",
    "sass": "1.26.10",
    "secure-password": "4.0.0",
    "ts-jest": "26.2.0",
    "typescript": "3.9.7",
    "wait-on": "5.2.0",
    "zod": "1.10.3"
  },
  "devDependencies": {
    "cypress": "5.1.0"
  },
  "private": true
}
